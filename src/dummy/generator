[
  {
    'repeat(5, 10)': {
      _id: '{{objectId()}}',
      name: {
        first: '{{firstName()}}',
        last: '{{surname()}}'
      },
      email(tags) {
        return `${this.name.first}.${this.name.last}@${this.company}${tags.domainZone()}`.toLowerCase();
      },
      phone: '{{phone()}}'
    }
  }
